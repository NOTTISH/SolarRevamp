function getNearestBasketball()
	local closestBall = nil
	local range = math.huge or 9e9
	for i, v in pairs(game:GetService("Workspace").Courts:GetDescendants()) do
		if string.find(v.Name, "Basketball")  and v.ClassName == "Part" then
			local distance = (game.Players.LocalPlayer.Character.Head.Position - v.Position).magnitude
			getgenv().distancee = distance
			if distance < range then
				range = distance
				closestBall = v
			end
		end
	end
	return closestBall
end
for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
			if v.Name == "Basketball" then
				firetouchinterest(game.Players.LocalPlayer.Character.LeftLowerArm, getNearestBasketball(), 0)
				task.wait()
				firetouchinterest(game.Players.LocalPlayer.Character.LeftLowerArm, getNearestBasketball(), 0)
			end
		end
